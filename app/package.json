        "create-react-class": "^15.6.3",
        "es6-promise": "^4.2.4",
        "form-data": "^2.3.2",
        "history": "^4.7.2",
        "immutable": "^3.8.2",
        "lodash": "^4.17.10",
        "moment": "^2.22.2",
        "react": "^16.4.2",
        "react-addons-css-transition-group": "^15.6.2",
